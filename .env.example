# Configuración del Bot de Telegram
BOT_TOKEN=tu_token_del_bot_aqui

# Configuración de Base de Datos MySQL
DB_HOST=localhost
DB_USER=tu_usuario_mysql
DB_PASSWORD=tu_contraseña_mysql
DB_NAME=antmaster

# API Keys (Opcionales pero recomendadas)
OPENAI_API_KEY=tu_clave_openai_aqui
GOOGLE_API_KEY=tu_clave_google_aqui
GOOGLE_CSE_ID=tu_id_custom_search_aqui

# Configuración de Administradores (IDs de Telegram separados por comas)
ADMIN_IDS=123456789,987654321

# URLs de APIs (No cambiar a menos que sea necesario)
ANTWIKI_API=https://www.antwiki.org/wiki/
ANTMAPS_API=https://antmaps.org/api
GOOGLE_SEARCH_API=https://www.googleapis.com/customsearch/v1

# Configuración del Bot
MAX_REQUESTS_PER_MINUTE=30
TIMEOUT=10
DEBUG=false 